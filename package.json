  },
  "homepage": "https://github.com/ghost-fa/Todo-list#readme",
  "devDependencies": {
    "react-scripts": "^1.1.4"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}
